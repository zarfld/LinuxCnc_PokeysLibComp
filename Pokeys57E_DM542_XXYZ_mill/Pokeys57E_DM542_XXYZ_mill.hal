setp   pokeys.0.PEv2.1.DIRSETUP        [JOINT_2]DIRSETUP
setp   pokeys.0.PEv2.1.DIRHOLD         [JOINT_2]DIRHOLD
setp   pokeys.0.PEv2.1.STEPLEN         [JOINT_2]STEPLEN
setp   pokeys.0.PEv2.1.STEPSPACE       [JOINT_2]STEPSPACE
setp   pokeys.0.PEv2.1.STEP-SCALE  	   [JOINT_2]STEP_SCALE
setp   pokeys.0.PEv2.1.MIN-LIMIT  	   [JOINT_2]MIN_LIMIT
setp   pokeys.0.PEv2.1.MAX-LIMIT  	   [JOINT_2]MAX_LIMIT
setp   pokeys.0.PEv2.1.HOME-OFFSET  	   [JOINT_2]HOME_OFFSET
setp   pokeys.0.PEv2.1.HOME-SEARCH-VEL  	   [JOINT_2]HOME_SEARCH_VEL
setp   pokeys.0.PEv2.1.HOME-LATCH-VEL  	   [JOINT_2]HOME_LATCH_VEL
setp   pokeys.0.PEv2.1.STEPGEN-MAXVEL        [JOINT_2]STEPGEN_MAXACCEL
setp   pokeys.0.PEv2.1.STEPGEN-MAXACCEL           [JOINT_2]STEPGEN_MAXVEL

# ---closedloop stepper signals---

net y-pos-cmd    <= joint.2.motor-pos-cmd
net y-vel-cmd    <= joint.2.vel-cmd
net y-output     <= pokeys.0.PEv2.1.joint-vel-cmd
net y-pos-fb     <= pokeys.0.PEv2.1.joint-pos-fb
